<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane id="mainPane" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.michaelgatesdev.OldLeaf.gui.controllers.tabs.TabMainController">

    <stylesheets>
        <!--<URL value="@../../css/tabs/TabMain.css" />-->
        <!--<URL value="@../../css/Flat.css" />-->
        <!--<URL value="@../../css/FlatColors.css" />-->
    </stylesheets>

    <center>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                    <children>
                        <Button fx:id="loadSaveButton" defaultButton="true" mnemonicParsing="false" prefHeight="40.0" prefWidth="246.0" text="Load garden.dat" />
                        <Button fx:id="saveSaveButton" disable="true" layoutX="287.0" layoutY="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="246.0" text="Save garden.dat" />
                    </children>
                </HBox>
            <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <Button fx:id="loadSaveButton1" defaultButton="true" mnemonicParsing="false" prefHeight="40.0" prefWidth="246.0" text="Load garden_plus.dat" />
                  <Button fx:id="saveSaveButton1" disable="true" layoutX="287.0" layoutY="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="246.0" text="Save garden_plus.dat" />
               </children>
            </HBox>
                <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                    <children>
                        <Button fx:id="loadRamButton" defaultButton="true" mnemonicParsing="false" prefHeight="40.0" prefWidth="246.0" text="Load acnlram.bin" />
                        <Button fx:id="saveRamButton" disable="true" layoutX="287.0" layoutY="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="246.0" text="Save acnlram.bin" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </center>
</BorderPane>
