<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1"
            id=""
            prefHeight="500.0"
            prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8.0.112"
            fx:controller="com.michaelgatesdev.OldLeaf.gui.controllers.tabs.TabMainController">

    <stylesheets>
        <!--<URL value="@../../css/tabs/TabMain.css" />-->
        <!--<URL value="@../../css/Flat.css" />-->
        <!--<URL value="@../../css/FlatColors.css" />-->
    </stylesheets>

    <center>
        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                    <children>
                        <Text fill="#00c61e" stroke="#00610a" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeType="OUTSIDE" text="Welcome">
                            <font>
                                <Font size="60.0"/>
                            </font>
                        </Text>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
                    <children>
                        <Button fx:id="loadButton" defaultButton="true" mnemonicParsing="false" prefHeight="40.0" prefWidth="246.0" text="Load Save"/>
                        <Button fx:id="exportButton" disable="true" layoutX="287.0" layoutY="40.0" mnemonicParsing="false" prefHeight="40.0"
                                prefWidth="246.0" text="Export Save"/>
                    </children>
                </HBox>
            </children>
        </VBox>
    </center>
</BorderPane>
